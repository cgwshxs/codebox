#include<iostream>
using namespace std;
typedef struct node{
  int num;
  struct node*next;
}node;
node*doublereverse(node*head){
  if(head==NULL||head->next==NULL)
    return head;
  node*pre=NULL;
  node*cur=head;
  node*last=head->next;
  head=head->next;
  while(cur!=NULL){
    last->next=cur;
    cur->next=last->next;
    pre=cur;
    cur=last;
    last=last->next->next;
  }
  return head;

}
int main(){
  node head,p1,p2,p3;
  head.num=1;
  head->next=&p1;
  p1->num=2;
  head->next=&p2;
  p2->num=3;
  head->next=&p3;
  p3->num=4;
  head->next=NULL;
}
